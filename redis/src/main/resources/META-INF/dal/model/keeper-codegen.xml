<?xml version="1.0" encoding="UTF-8"?>
<model>
  <entity name="xpipe_meta" root="true">
    <entity-ref name="cluster_meta" type="list" names="clusters" />
  </entity>
  <entity name="cluster_meta">
    <attribute name="id" value-type="String" />
    <entity-ref name="shard_meta" type="list" names="shards" />
  </entity>
  <entity name="shard_meta">
    <attribute name="id" value-type="String" />
    <entity-ref name="keeper_meta" type="list" names="keepers" />
    <entity-ref name="redis_meta" type="list" names="redises" />
  </entity>
  <entity name="keeper_meta">
    <attribute name="id" value-type="String" />
    <attribute name="ip" value-type="String" />
    <attribute name="port" value-type="int" />
    <attribute name="active" value-type="boolean" />
  </entity>
  <entity name="redis_meta">
    <attribute name="ip" value-type="String" />
    <attribute name="port" value-type="int" />
    <attribute name="master" value-type="boolean" />
  </entity>
</model>

